msgctxt "#32133"
msgid "Use local cache"
msgstr "Use local cache"

msgctxt "#32134"
msgid "Saves all films locally. Opening a folder for the second time is instant (under 1 second). The cache is refreshed automatically once a day in the background."
msgstr "Saves all films locally. Opening a folder for the second time is instant (under 1 second). The cache is refreshed automatically once a day in the background."

msgctxt "#32135"
msgid "Add new content only"
msgstr "Add new content only"

msgctxt "#32136"
msgid "Checks for new films and adds them to the cache. Existing data is kept. Only new films get TMDB metadata fetched."
msgstr "Checks for new films and adds them to the cache. Existing data is kept. Only new films get TMDB metadata fetched."

msgctxt "#32137"
msgid "TMDB Cache"
msgstr "TMDB Cache"

msgctxt "#32138"
msgid "Cache validity"
msgstr "Cache validity"

msgctxt "#32139"
msgid "How long TMDB metadata is kept before being re-downloaded. 'Never delete' keeps data indefinitely."
msgstr "How long TMDB metadata is kept before being re-downloaded. 'Never delete' keeps data indefinitely."

msgctxt "#32140"
msgid "Show cache info"
msgstr "Show cache info"

msgctxt "#32141"
msgid "Shows number of stored entries, age of oldest and newest entry, days remaining until expiry, and cache file size."
msgstr "Shows number of stored entries, age of oldest and newest entry, days remaining until expiry, and cache file size."

msgctxt "#32142"
msgid "Update TMDB metadata"
msgstr "Update TMDB metadata"

msgctxt "#32143"
msgid "Fetches TMDB metadata for all films already in the local Stalker cache. Does not re-download Stalker data. Films already cached in TMDB are skipped."
msgstr "Fetches TMDB metadata for all films already in the local Stalker cache. Does not re-download Stalker data. Films already cached in TMDB are skipped."

msgctxt "#32144"
msgid "Delete TMDB cache"
msgstr "Delete TMDB cache"

msgctxt "#32145"
msgid "Deletes all locally stored TMDB metadata. Data will be re-downloaded the next time you open a folder."
msgstr "Deletes all locally stored TMDB metadata. Data will be re-downloaded the next time you open a folder."

